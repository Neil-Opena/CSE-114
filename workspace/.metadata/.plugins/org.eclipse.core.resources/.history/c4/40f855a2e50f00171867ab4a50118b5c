
public class Person {

	private String name;
	private String address;
	private String phoneNumber;
	private int age;
	private String emailAddress;
	
	public int getAge(){
		return age;
	}
	
	public Person(String a, String b, String c, int d, String e){
		name = a;
		address = b;
		phoneNumber = c;
		age = d;
		emailAddress = e;
	}
}
