
public class Solution {

    public static void main(String[] args) {
       int[] myArray = {1,2,3,2,1};
       if(isReverse(myArray))
    	   System.out.println();
    
    }
    
    public static boolean isReverse(int[] arr){
    	for(int i = 0; i < arr.length; i++){
    		if(arr[i] != arr[arr.length - 1 - i]){
    			return false;
    		}
    	}
    	
    	return true;

    }
}