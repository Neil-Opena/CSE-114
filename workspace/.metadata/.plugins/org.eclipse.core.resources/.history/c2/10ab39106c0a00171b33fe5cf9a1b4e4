public class MyPoint {

	private double x;
	private double y;
	
	public MyPoint(){
		x = 0;
		y = 0;
	}
	
	public MyPoint(double x, double y){
		this.x = x;
		this.y = y;
	}
	
	public double getX(){
		return x;
	}
	
	public double getY(){
		return y;
	}
}
