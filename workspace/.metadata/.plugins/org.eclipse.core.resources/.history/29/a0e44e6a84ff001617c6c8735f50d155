
public class Solution {

    public static void main(String[] args) {
       int[] one = {5,88,6,17,39,31,100,99,19,4,98,22,77,10};
       int[] two = buildHistogram(one);
       
       for(int i = 0; i < two.length; i++){
    	   System.out.print(two[i] + " ");
       }
    }
    
    public static int[] buildHistogram(int[] array){
    	int[] histogram = new int[10];
    	for(int i = 0; i < array.length; i++){
    		if(array[i] <= 9){
    			histogram[0]++;
    		}else if(array[i] <= 19){
    			histogram[1]++;
    		}else if(array[i] <= 29){
    			histogram[2]++;
    		}else if(array[i] <= 39){
    			histogram[3]++;
    		}else if(array[i] <= 49){
    			histogram[4]++;
    		}else if(array[i] <= 59){
    			histogram[5]++;
    		}else if(array[i] <= 69){
    			histogram[6]++;
    		}else if(array[i] <= 79){
    			histogram[7]++;
    		}else if(array[i] <= 89){
    			histogram[8]++;
    		}else{
    			histogram[9]++;
    		}
    	}
    	
    	return histogram;
    }
    
    public static void printNumPattern(){
    	for(int i = 1; i <= 9; i++){
     	   for(int j = 1; j < i; j++){
     		   System.out.print(" ");
     	   }
     	   
     	   for(int j = i; j<= 9; j++){
     		   System.out.print(j);
     	   }
     	   
     	   for(int j = 8; j >= i; j--){
     		   System.out.print(j);
     	   }
     	   
     	   System.out.println();
        }
    }
    
}