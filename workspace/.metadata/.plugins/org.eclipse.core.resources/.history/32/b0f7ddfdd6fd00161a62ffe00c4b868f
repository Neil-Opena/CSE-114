import java.util.*;
public class PrintingDistinctNumbers {

	public static void main(String[] arg){
		
		Scanner myScanner = new Scanner(System.in);
		System.out.print("Enter 10 integers: ");
		int first = myScanner.nextInt();
		int second = myScanner.nextInt();
		int third = myScanner.nextInt();
		int fourth = myScanner.nextInt();
		int fifth = myScanner.nextInt();
		int sixth = myScanner.nextInt();
		int seventh = myScanner.nextInt();
		int eight = myScanner.nextInt();
		int ninth = myScanner.nextInt();
		int tenth = myScanner.nextInt();
		boolean notFound = true;
		int[] myArray = {first, second, third, fourth, fifth, sixth, seventh, eight, ninth, tenth};
		int[] newArray = new int[10];
		for(int i = 0; i < myArray.length; i ++){
			for(int j = 0; j < myArray.length; j++){
				if((i!=j)&&(myArray[i] == myArray[j])){
					notFound = false;
				}
			}
			
			if(notFound){
				newArray[i] = myArray[i];
			}
		}
	
		printElements(myArray);
		System.out.println();
		printElements(newArray);
	}
	
	public static void printElements(int[] arr){
		for(int i = 0; i<arr.length;i++){
			System.out.print(arr[i] + " ");
		}
	}
	
}
